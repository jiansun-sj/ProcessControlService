		  <OutParameter Name="FaultCode" Type="int16" />
          <ActionItem Type="ReadTag" Tag="FE110_TP.To_MEB.Result" OutParameter="ResultCode" />	  
          <ActionItem Type="ReadTag" Tag="FE110_TP.To_MEB.Fault_Code" OutParameter="FaultCode" />
        </Action>	
		
		<!--发送完成-->
		<Action Type="TagsAction" Name="OfflineTP_SetComplete">
          <ActionItem Type="WriteTag" Tag="FE110_TP.From_MEB.Complete" ConstValue="True" />
        </Action>
		<!--设置车辆状态为已下线-->
		<Action Type="SetCarStateAction" Name="OfflineTP_SetCarState">
          <InParameter Name="Line" Type="String" />
          <InParameter Name="Station" Type="String" />
          <InParameter Name="TPS" Type="String" />
          <InParameter Name="State" Type="int16"/>
          <InParameter Name="DBConnection" Type="String" />
