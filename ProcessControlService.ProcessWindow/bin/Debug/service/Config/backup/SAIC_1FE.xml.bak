		<!--读取接收结果-->
		<Action Type="TagsAction" Name="OnlineTP_ReadResultCode">
          <OutParameter Name="ResultCode" Type="int16" />
		  <OutParameter Name="FaultCode" Type="int16" />
          <ActionItem Type="ReadTag" Tag="FE010_TP.To_MEB.Result" OutParameter="ResultCode" />	  
          <ActionItem Type="ReadTag" Tag="FE010_TP.To_MEB.Fault_Code" OutParameter="FaultCode" />
        </Action>	
		
		<!--发送完成-->
