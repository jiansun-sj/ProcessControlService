	</Actions>
	</Resource>
	
	<!--FE010订单请求-->
    <Resource Type="Process" Name="FE010订单请求" Enable="true" LogOutput="true" >
      <Condition Type="SingleTagOnCondition" Name="订单请求" Container="FE1Machine" Tag="FE010_Order.To_MEB.Data_Request" />
      <ProcessParameters>
	    <ProcessParameter Name="Factory" Type="int16" DefaultValue="1"/>
		<!--车辆状态3已下发-->
        <ProcessParameter Name="State" Type="int16" DefaultValue="3"/>
        <ProcessParameter Name="Station" Type="string"/>
        <ProcessParameter Name="Line" Type="string"/>
        <ProcessParameter Name="RequestID" Type="int16"/>
        <ProcessParameter Name="TPS" Type="string" />
        <ProcessParameter Name="FaultCode" Type="int16" />
        <ProcessParameter Name="ReceiveCode" Type="int16" />
        <ProcessParameter Name="DBConnection" Type="string" DefaultValue="SAICDatabase" />
      </ProcessParameters>
      <Steps>
        <Step Index="1" Name="读取线体和工位" Wait="5" NextStepIndex="2">
          <StepAction Container="FE1Machine" Action="FE010ReadStation">
            <OutParameter ProcessParameter="Station" ActionParameter="Station" />
            <OutParameter ProcessParameter="Line" ActionParameter="Line" />
            <OutParameter ProcessParameter="RequestID" ActionParameter="RequestID" />
			<ShowParameter ProcessParameter="RequestID"/>
          </StepAction>
        </Step>
        <Step Index="2" Name="查询TPS" Wait="5" NextStepIndex="3">
          <StepAction Container="FE1Machine" Action="FE010GetTPS">
            <InParameter ProcessParameter="Line" ActionParameter="Line" />
            <InParameter ProcessParameter="Station" ActionParameter="Station" />
			<InParameter ProcessParameter="DBConnection" ActionParameter="DBConnection" />
			<InParameter ProcessParameter="Factory" ActionParameter="Factory" />
            <OutParameter ProcessParameter="TPS" ActionParameter="TPS" />
          </StepAction>
        </Step>
		<Step Index="3" Name="写TPS" Wait="5" NextStepIndex="20">
          <StepAction Container="FE1Machine" Action="FE010WriteTPS">
			<InParameter ProcessParameter="TPS" ActionParameter="TPS" />
			<InParameter ProcessParameter="RequestID" ActionParameter="RequestID" />
          </StepAction>
		   
        </Step>
		 <Step Index="15" Name="发送完成信号" Wait="5" NextStepIndex="-1">
          <StepAction Container="FE1Machine" Action="FE010SetComplete">
          </StepAction>
        </Step>
        <Step Index="20" Name="TrueWaiting" NextStepIndex="5" >
    <StepAction Container="Common" Action="WaitAction">
     <InParameter ActionParameter="WaitTime" ConstValue="6000"/>
    </StepAction>
   </Step>  <Step Index="5" Name="检查接收" Wait="25" NextStepIndex="6">
          <StepAction Container="FE1Machine" Action="FE010CheckRecive">
          </StepAction>
        </Step>
		<Step Index="6" Name="读取接收代码和错误代码" Wait="5" NextStepIndex="7">
          <StepAction Container="FE1Machine" Action="FE010ReadReceiveCode">
            <OutParameter ProcessParameter="ReceiveCode" ActionParameter="ReceiveCode" />
			<OutParameter ProcessParameter="FaultCode" ActionParameter="FaultCode" />
			<ShowParameter ProcessParameter="ReceiveCode"/>
			<ShowParameter ProcessParameter="FaultCode"/>
          </StepAction>
		  <StepChecks>	
			<!--成功-->
            <StepCheck ProcessParameter="ReceiveCode" ConstValue="1" NextStepIndex="7" />
			<!--失败-->
            <StepCheck ProcessParameter="ReceiveCode" ConstValue="2" NextStepIndex="-1" />
          </StepChecks>
        </Step>
		<Step Index="7" Name="更新订单位置" Wait="5" NextStepIndex="8">
          <StepAction Container="FE1Machine" Action="FE010UpdateScreenTPS">
            <InParameter ProcessParameter="DBConnection" ActionParameter="DBConnection" />
            <InParameter ProcessParameter="Line" ActionParameter="Line" />
            <InParameter ProcessParameter="Station" ActionParameter="Station" />
            <InParameter ProcessParameter="Factory" ActionParameter="Factory" />
            <InParameter ProcessParameter="TPS" ActionParameter="TPS" />
          </StepAction>
        </Step>
		<Step Index="8" Name="设置车辆状态为已下发" Wait="5" NextStepIndex="9">
          <StepAction Container="FE1Machine" Action="FE010SetCarState">
            <InParameter ProcessParameter="DBConnection" ActionParameter="DBConnection" />
            <InParameter ProcessParameter="Station" ActionParameter="Station" />
            <InParameter ProcessParameter="State" ActionParameter="State" />
            <InParameter ProcessParameter="Line" ActionParameter="Line" />
            <InParameter ProcessParameter="TPS" ActionParameter="TPS" />
          </StepAction>
        </Step>
        <Step Index="9" Name="发送完成" Wait="5" NextStepIndex="10">
          <StepAction Container="FE1Machine" Action="FE010SetComplete">
          </StepAction>
        </Step>
        <Step Index="10" Name="订单下发记录数据库" Wait="5" NextStepIndex="-1">
          <StepAction Container="FE1Machine" Action="FE010OrderSendLog">
            <InParameter ProcessParameter="DBConnection" ActionParameter="DBConnection" />
            <InParameter ProcessParameter="Station" ActionParameter="Station" />
            <InParameter ProcessParameter="Line" ActionParameter="Line" />
            <InParameter ProcessParameter="TPS" ActionParameter="TPS" />
            <InParameter ProcessParameter="FaultCode" ActionParameter="FaultCode" />
          </StepAction>
        </Step>
      </Steps>
    </Resource>
   
</Resources>
</root>

