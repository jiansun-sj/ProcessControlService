          <InParameter Name="Station" Type="String" />
          <InParameter Name="TPS" Type="String" />
          <InParameter Name="State" Type="int16"/>
          <InParameter Name="DBConnection" Type="String" />
        </Action>
		<!--过点数据库记录-->
		<Action Type="TPLogAction" Name="OnlineTP_Log">
          <InParameter Name="Station" Type="String" />
          <InParameter Name="Line" Type="String" />
          <InParameter Name="BSN" Type="String" />
          <InParameter Name="DBConnection" Type="String" />
          <InParameter Name="FaultCode" Type="int16" />
        </Action>
	
<!--1FE下线过点Actions-->
		<!--读取过点工位和BSN-->
		<Action Type="TagsAction" Name="OfflineTP_ReadBSN">
          <OutParameter Name="Station" Type="String" />
          <OutParameter Name="BSN" Type="String"/>
